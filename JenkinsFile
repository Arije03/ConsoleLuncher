pipeline {
  agent any 
    stages {
      stage('Checkout Codebase') {
        steps { 
           cleanWs()
           checkout scm: [$class: 'GitSCM',userRemoteConfigs: [[credentialsId: 'GitHubSShkey',url: 'git@github.com:mnorm88/multithreading-example-1.git']]] 
         } 
       } 
      
       stage('Build'){
            steps{
                bat 'mkdir lib'
                bat curl.exe -o index.html https://www.meteo.tn
                
               
            }
        }

     

        
    }

}
